<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.racekit.uicontrollers.StartupController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>

  <Label fx:id="appName" text="RaceKit">
      <font>
         <Font size="40.0" />
      </font></Label>
   <VBox alignment="CENTER">
      <children>
        <Button onAction="#onStartTelemetry" text="Start" />
         <Button alignment="CENTER" mnemonicParsing="false" onAction="#onEditConnParams" text="UDP/IP Settings">
            <VBox.margin>
               <Insets />
            </VBox.margin></Button>
      </children>
   </VBox>
</VBox>
